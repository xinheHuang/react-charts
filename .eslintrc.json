{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "import/prefer-default-export": [
      "warn"
    ],
    "no-unused-vars": "warn",
    "max-len": "warn",
    "no-mixed-operators": "off",
    "react/prefer-stateless-function": "warn",
    "react/require-default-props": "warn"
  }
}